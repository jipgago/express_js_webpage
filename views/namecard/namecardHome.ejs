<h1>NameCard </h1>
<table class="table table-sm table-bordered" style="width : 600px">
    <% var i=0; while(i < results.length) { %>
        <tr>
            <td class="td td-sm">
                <%=results[i].name%>
            </td>
            <td>
                <%=results[i].description%>
            </td>
        </tr>
        <% i++; } %>
</table>